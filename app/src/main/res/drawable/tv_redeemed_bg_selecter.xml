<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="@dimen/_8sdp" android:topRightRadius="@dimen/_8sdp" />
            <solid android:color="@android:color/transparent" />
            <stroke android:width="@dimen/_2sdp" android:color="@color/yellow_tabs" />
        </shape>
    </item>
    <item android:color="@color/blue_11_tans30">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners android:bottomRightRadius="@dimen/_8sdp" android:topRightRadius="@dimen/_8sdp" />
                    <solid android:color="@android:color/transparent" />
                    <stroke android:width="@dimen/_2sdp" android:color="@color/disabled_tab_border" />
                </shape>
            </item>
            <item android:bottom="@dimen/_minus4sdp" android:right="@dimen/_minus4sdp" android:top="@dimen/_minus4sdp">
                <shape>
                    <solid android:color="@android:color/transparent" />
                    <stroke android:width="@dimen/_2sdp" android:color="@color/yellow_tabs" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>